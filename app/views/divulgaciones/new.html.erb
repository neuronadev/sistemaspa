

<% if @tipo == 'ADP' %>
    <div>
      <h5 class="text-primary">Registro - Artículo de Divulgación en Periódico</h5>
    </div>
    <%= render 'form_periodico', actividad: @actividad %>
<% end %>

<% if @tipo == 'CASA' %>
   <div>
     <h5 class="text-success">Actividades Institucionales</h5>
   </div>
  <%= render 'form_casa_abierta', actividad: @actividad %>
<% end %>